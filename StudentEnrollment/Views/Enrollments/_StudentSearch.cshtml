@model IEnumerable<StudentEnrollment.Models.Student>

@if (Model.Count() > 0)
{
    <p><b>Student Search Results:</b></p>
    foreach (var item in Model)
    {
        <li> @Html.DisplayFor(student => item.LastName), @Html.DisplayFor(student => item.FirstName) </li>
    }
}
else
{
    <p><b>Student Search Results:</b><i>No Results found!</i></p>
}
