@model IEnumerable<StudentEnrollment.Models.Course>

@if (Model.Count() > 0)
{
    <p><b>Course Search Results:</b></p>
    foreach (var item in Model)
    {
        <li> @Html.DisplayFor(course => item.Title): @Html.DisplayFor(course => item.Description) </li>
    }
}
else
{
    <p><b>Course Search Results:</b><i>No Results found!</i></p>
}
